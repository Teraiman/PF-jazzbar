<h3 class="text-center p-5">Artist一覧</h3>
<div class="container">

  <div class="row">
    <table class="table table-hover">
      <!--each文を使う-->
      <% @artists.each do |artist| %>
          <tr>
              <%= link_to edit_admin_artist(artist.id) do %>
                <%= artist.name %>
              <% end %>
            <td><%= @part.part_name %></td>
            <td>
            </td>
          </tr>
      <% end %>
      <!--each文を使う-->
    </table>
  </div>
  <%= paginate @artist %>
</div>
